{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data:",
    "cache-control": "must-revalidate, max-age=6000"
  },
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}
